<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="study.dao.SheepMapper">
    <select id="findByXml" resultType="study.entity.Sheep">
        SELECT *from sheep where age =#{age}
    </select>
